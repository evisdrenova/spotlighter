{
  "name": "spotlighter",
  "displayName": "Spotlight for Code in Vscode",
  "description": "Dims all code outside the current function with your cursor.",
  "version": "0.0.1",
  "publisher": "evisdrenova",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:rust"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "spotlighter.enable",
        "title": "Enable Rust Function Spotlighter"
      },
      {
        "command": "spotlighter.debugSymbols",
        "title": "Debug: Log Rust Symbols"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p .",
    "watch": "tsc -watch -p .",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/node": "^16.0.0",
    "@types/vscode": "^1.70.0",
    "typescript": "^4.9.5",
    "vsce": "^2.11.0"
  }
}
